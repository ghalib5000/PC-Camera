<Window x:Class="PC_Camera.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PC_Camera"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Res" HorizontalAlignment="Left" Height="142" Margin="10,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="123"/>
        <TextBlock HorizontalAlignment="Left" Margin="164,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="85" Height="22"><Run Text="Time in Mins"/><Run Text=":"/></TextBlock>
        <TextBox x:Name="mins_num" HorizontalAlignment="Left" Height="23" Margin="276,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="set_mins" Content="Set" HorizontalAlignment="Left" Margin="418,26,0,0" VerticalAlignment="Top" Width="53" Height="22" Click="Set_mins_Click"/>
        <TextBox x:Name="time_out" HorizontalAlignment="Left" Height="215" Margin="10,195,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="123"/>


    </Grid>
</Window>
